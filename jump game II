class Solution:
    def jump(self, nums: List[int]) -> int:
        l = len(nums) #length
        jumps = 0 #total jumps
        farthest = 0 #max reach
        currentEnd = 0 #boundry of current jump
        
        for i in range(l-1):
            farthest = max(farthest, i + nums[i])
            if i == currentEnd: #increase jump only when cross boundry
                jumps += 1
                currentEnd = farthest
        return jumps
